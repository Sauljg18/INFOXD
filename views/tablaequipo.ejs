<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="resources/estilo/tabla.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>
    <title>Equipos</title>
  
  </head>
  <body>
    <div class="fondo1"></div>

    </div>
    <div class = "fondo2" id="estrella-12">
    </div>
    
    <div class="container">
<table class="table" >
    <thead>
        
          
        <tr class="bg-primary">
            <th scope="col">Nombre</th>
            <th scope="col">indentificiacion</th>
            <th scope="col">Correo</th>
            <th scope="col">Telefono</th>
            <th scope="col">Responsable</th>
            <th scope="col">Direccion</th>
            <th scope="col">Region</th>
            <th scope="col">Ciudad</th>
            <th scope="col">Estado</th>

        </tr>
        
    
    </thead>
    <tbody>
            <% results.forEach((cliente) => { %>
                
          
        <tr>
            <td scope="row" class="table-primary"><%= cliente.nombre %></td>
            <td scope="row" ><%= cliente.indentificiacion %></td>
            <td scope="row" class="table-primary"><%= cliente.correocliente %></td>
            <td scope="row" ><%= cliente.telefonocorp %></td>
            <td scope="row" ><%= cliente.responsable %></td>
            <td scope="row" class="table-primary"><%= cliente.direccion %></td>
            <td scope="row" class="table-primary"><%= cliente.region %></td>
            <td scope="row" class="table-primary"><%= cliente.ciudad %></td>
            <td scope="row" class="table-primary"><%= cliente.estado %></td>

            <td>
              <a href="/edit" class="btn btn-outline-info">editar</a>
              <a href="/edit" class="btn btn-outline-info">borrar</a>
            </td>
        </tr>
        <%  }) %>
    </tbody>
           
</table>
</div>
<button> <a class="nav*links" href="registrocliente">crear</a></button>


  </body>
</html>